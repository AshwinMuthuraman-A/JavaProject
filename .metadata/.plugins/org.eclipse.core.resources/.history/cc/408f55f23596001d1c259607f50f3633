package com.example.demo.services;

import com.example.demo.Exceptions.UserCollectionException;
import com.example.demo.model.Student;

import jakarta.validation.ConstraintViolationException;

public interface UserService {
	public void createStudent(Student s) throws ConstraintViolationException, UserCollectionException;
	String update(String text, Student s);
	Student remove(String text);
}
